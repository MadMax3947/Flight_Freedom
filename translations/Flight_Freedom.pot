msgstr ""

#. [message]: speaker=Malakar
#: Flight_Freedom/scenarios/08_Landfall.cfg:1204
msgid "Let us go!"
msgstr ""

#. [message]: speaker=Malakar
#: Flight_Freedom/scenarios/08_Landfall.cfg:1263
